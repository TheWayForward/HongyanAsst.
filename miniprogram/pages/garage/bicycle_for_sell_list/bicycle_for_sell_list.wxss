
.bicycle_block{
  padding: 15rpx;
}

.bicycle_inner_block{
  background-color: white;
  border-radius: 10rpx;
  padding-bottom: 15rpx;
}

.bicycle_inner_block:active{
  transform: scale(0.95);
  opacity: 0.7;
  transition: 0.1s ease;
}

.title{
  padding: 0 10rpx;
  text-align: justify;
  word-wrap: break-word;
}
